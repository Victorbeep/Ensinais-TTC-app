{
  "expo": {
    "name": "Ensinais",
    "slug": "Ensinais",
    "owner": "liankkjk",
    "version": "1.0.0",
    "scheme": "ensinais",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": ["ios", "android"],
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#F2F2F2"
        }
      ],
      [
        "expo-video",
        {
          "supportsPictureInPicture": true
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.ensinais.ios",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ensinais.android"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dafde0ff-9d21-4eda-9591-e27a8670a270"
      }
    }
  }
}
